{
    "name": "chaser",
    "version": "0.4.2",
    "description": "Javascript variable chaser",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "build": "./node_modules/.bin/tsc -p ./tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepublish": "npm test && npm run lint && npm run build",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "example:browser": "webpack-dev-server --mode development",
        "example:node": "nodemon",
        "deploy": "gh-pages -d examples/dist",
        "publish-demo": "yarn build && yarn run deploy",
        "v:patch": "npm version patch --force",
        "v:minor": "npm version minor --force",
        "v:major": "npm version major --force"
    },
    "repository": "https://github.com/Alireza29675/chaser.git",
    "author": "Alireza Sheikholmolouki",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@types/jest": "^24.0.15",
        "@types/node": "12.6.9",
        "@types/react": "16.8.24",
        "@types/react-dom": "^16.8.5",
        "babel-loader": "^8.0.6",
        "css-loader": "^2.1.1",
        "gh-pages": "^2.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.8.0",
        "prettier": "^1.18.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "3.0.1",
        "style-loader": "^0.23.1",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.5.3",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.3",
        "webpack-dev-server": "^3.7.1",
        "@babel/preset-react": "^7.0.0"
    }
}
